<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="480dp"
    android:layout_height="800dp"
    tools:context=".IncomeInsert">

    <TextView
        android:id="@+id/textView6_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:text="收入金额"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView8_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:text="收入类型"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7_update" />

    <TextView
        android:id="@+id/textView10_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="40dp"
        android:text="备注"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9_update" />

    <TextView
        android:id="@+id/textView9_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="40dp"
        android:text="付款方"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8_update" />

    <TextView
        android:id="@+id/textView7_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:text="收入时间"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6_update" />

    <EditText
        android:id="@+id/income_money_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintStart_toEndOf="@+id/textView6_update"
        app:layout_constraintTop_toTopOf="@+id/textView6_update" />

    <EditText
        android:id="@+id/income_handler_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/textView9_update"
        app:layout_constraintTop_toTopOf="@+id/textView9_update" />

    <EditText
        android:id="@+id/income_mark_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/textView10_update"
        app:layout_constraintTop_toTopOf="@+id/textView10_update" />

    <Button
        android:id="@+id/income_updatebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="id更新"
        app:layout_constraintStart_toEndOf="@+id/income_findidbutton"
        app:layout_constraintTop_toTopOf="@+id/income_findidbutton" />

    <Button
        android:id="@+id/income_deletebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="id删除"
        app:layout_constraintStart_toEndOf="@+id/income_updatebutton"
        app:layout_constraintTop_toTopOf="@+id/income_updatebutton" />

    <Button
        android:id="@+id/income_findidbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="id查询"
        app:layout_constraintStart_toEndOf="@+id/income_idvalue"
        app:layout_constraintTop_toTopOf="@+id/income_idvalue" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="32dp"
        android:text="id号"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/income_idvalue"
        android:layout_width="64dp"
        android:layout_height="41dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toTopOf="@+id/textView13" />

    <Spinner
        android:id="@+id/spinner_update"
        android:layout_width="217dp"
        android:layout_height="42dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="7dp"
        app:layout_constraintStart_toEndOf="@+id/textView8_update"
        app:layout_constraintTop_toTopOf="@+id/textView8_update" />

    <TextView
        android:id="@+id/textView2_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginTop="16dp"
        android:text="（双击插入时间）"
        app:layout_constraintStart_toEndOf="@+id/income_time_update"
        app:layout_constraintTop_toTopOf="@+id/income_time_update" />

    <EditText
        android:id="@+id/income_time_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintStart_toEndOf="@+id/textView7_update"
        app:layout_constraintTop_toTopOf="@+id/textView7_update" />

</androidx.constraintlayout.widget.ConstraintLayout>