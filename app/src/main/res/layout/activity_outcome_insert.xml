<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="480dp"
    android:layout_height="800dp"
    tools:context=".IncomeInsert">

    <TextView
        android:id="@+id/outcome_insert_textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="30dp"
        android:text="支出金额"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/outcome_insert_textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="60dp"
        android:text="支出时间"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outcome_insert_textView1" />

    <EditText
        android:id="@+id/outcome_insert_money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintStart_toEndOf="@+id/outcome_insert_textView1"
        app:layout_constraintTop_toTopOf="@+id/outcome_insert_textView1" />

    <TextView
        android:id="@+id/outcome_insert_textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="60dp"
        android:text="支出类型"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outcome_insert_textView2" />

    <TextView
        android:id="@+id/outcome_insert_textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="60dp"
        android:text="支出地点"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outcome_insert_textView3" />

    <TextView
        android:id="@+id/outcome_insert_textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="56dp"
        android:text="备注"
        android:textColor="#2A3E52"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outcome_insert_textView4" />

    <EditText
        android:id="@+id/outcome_insert_handler"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/outcome_insert_textView4"
        app:layout_constraintTop_toTopOf="@+id/outcome_insert_textView4" />

    <EditText
        android:id="@+id/outcome_insert_mark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/outcome_insert_textView5"
        app:layout_constraintTop_toTopOf="@+id/outcome_insert_textView5" />

    <Button
        android:id="@+id/outcome_insert_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:layout_marginTop="52dp"
        android:text="新增"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outcome_insert_mark" />


    <Button
        android:id="@+id/outcome_insert_clear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginTop="52dp"
        android:text="清空输入栏"
        app:layout_constraintStart_toEndOf="@+id/outcome_insert_button"
        app:layout_constraintTop_toBottomOf="@+id/outcome_insert_mark" />

    <Spinner
        android:id="@+id/outcome_insert_spinner"
        android:layout_width="217dp"
        android:layout_height="42dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="7dp"
        app:layout_constraintStart_toEndOf="@+id/outcome_insert_textView3"
        app:layout_constraintTop_toTopOf="@+id/outcome_insert_textView3" />

    <TextView
        android:id="@+id/outcome_insert_textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="16dp"
        android:text="（双击插入时间）"
        app:layout_constraintStart_toEndOf="@+id/outcome_insert_time"
        app:layout_constraintTop_toTopOf="@+id/outcome_insert_time" />

    <EditText
        android:id="@+id/outcome_insert_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintStart_toEndOf="@+id/outcome_insert_textView2"
        app:layout_constraintTop_toTopOf="@+id/outcome_insert_textView2" />

</androidx.constraintlayout.widget.ConstraintLayout>